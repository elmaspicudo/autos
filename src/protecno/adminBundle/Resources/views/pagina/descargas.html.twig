							<p class="work-item-actions">
								<a class="colorbox" href="{{ asset('images/slider/'~ descarga.imagenes.0.path)}}"><i class="fa fa-search"></i></a>
								
							</p>
						</div>
						<img src="{{ asset('images/slider/'~ descarga.imagenes.0.path)}}" alt="">
					</li>
					
				 {% endfor %}
                        </ul>
                {% endif %}
			</div>
